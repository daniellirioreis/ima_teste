<%= simple_form_for(@client) do |f| %>
<div class="row-fluid">
  <div class="span10">
    <div class="span8"><%= f.input :name, input_html: { class: 'span10' } %></div>
  </div>
</div>
<div class="row-fluid">
  <div class="span10">
    <div class="span8"><%= f.input :email, input_html: { class: 'span10' } %></div>
  </div>
</div>
<div class="row-fluid">
  <div class="span3">
    <div class="span8"><%= f.input :phone, input_html: { class: 'span10' } %></div>
  </div>
  <div class="span3">
    <div class="span8"><%= f.input :cel_phone, input_html: { class: 'span10' } %></div>
  </div>

</div>

<div class="form-actions">
  <%= f.button :submit, :class => "btn btn-success" %>
  <%= link_to 'Voltar', clients_path, :class => "btn"  %>
</div>
<% end %>
