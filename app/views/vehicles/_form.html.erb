<%= simple_form_for(@vehicle) do |f| %>
<div class="row-fluid">
  <div class="span10">
    <div class="span3"><%= f.input :license_plate, input_html: {maxlength: 7, class: 'span10' } %></div>
		<div class="span9"><%= f.input :type_vehicle, as: :string, input_html: {class: 'span10'} %></div>
  </div>
</div>
<div class="row-fluid">
  <div class="span3">
    <%= f.input :client_id, collection: Client.all, input_html: { class: 'span12'} %>
  </div>
</div>
<div class="row-fluid">
  <div class="span10">
	 <div class="span10"><%= f.input :description, input_html: {rows: 10, class: 'span10' } %></div>
  </div>
</div>
<div class="form-actions">
  <%= f.button :submit, :class => "btn btn-success" %>
  <%= link_to 'Voltar', vehicles_path, :class => "btn"  %>
</div>
<% end %>
