<%= simple_form_for(@schedule) do |f| %>
<div class="row-fluid">
  <div class="span7">
    <%= f.input :vehicle_id, collection: Vehicle.all, input_html: { class: 'span12'} %>
  </div>
  <div class="span3">
    <%= f.input :technical_consultant_id, collection: TechnicalConsultant.all, input_html: { class: 'span12'} %>
  </div>

</div>

<div class="row-fluid">
  <div class="span5"><%= f.input :date_schedule, input_html: { class: 'span4' } %></div>
</div>

<div class="row-fluid">
  <div class="span5"><%= f.input :hour_schedule, input_html: { class: 'span4' } %></div>
</div>
<br>
<div class="row-fluid">
  <div class="span10">
	 <div class="span10"><%= f.input :description_service, input_html: {rows: 10, class: 'span10' } %></div>
  </div>
</div>

<div class="form-actions">
  <%= f.button :submit, :class => "btn btn-success" %>
  <%= link_to 'Voltar', products_path, :class => "btn"  %>
</div>
<% end %>
